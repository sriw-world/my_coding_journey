
from queue import Queue 
def insertbinarytree(root,val):
    if root is None:
        rootNode = NewNode
    else:
        q = Queue()
        q.put(root)
        while not q.empty():
            for i in range(q.qsize()):
                root = q.get()
                if root.left is not None:
                    q.put(root.left)
                else:
                    root.left = NewNode(val)
                    return
                if root.right is not None:
                    q.put(root.right)
                else:
                    root.right = NewNode(val)
                    return
                


from queue import Queue 
def getdeepestnodebinarytree(root,val):
    if root is None:
        return
    else:
        q = Queue()
        q.put(root)
        while not q.empty():
            root = q.get()
            if root.left is not None:
                q.put(root.left)
            if root.right is not None:
                q.put(root.right)
        val = root.val
        #return val
        return root
