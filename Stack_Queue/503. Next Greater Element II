class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        ans = [-1]*len(nums)
        st = []
        for idx,num in enumerate(nums):
            while st and nums[st[-1]] < nums[idx]:
                ans[st.pop()] = num

            st.append(idx)
        for num in nums:
            while st and nums[st[-1]] < num:
                ans[st.pop()] = num
        return ans

